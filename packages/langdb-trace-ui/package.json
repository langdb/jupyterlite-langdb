{
    "name": "@langdb/trace-ui",
    "version": "0.1.0",
    "description": "UI to show langdb traces for notebook cells",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/langdb/jupyterlite-langdb",
    "bugs": {
        "url": "https://github.com/langdb/jupyterlite-langdb/issues"
    },
    "license": "BSD-3-Clause",
    "author": "Langdb",
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "src/**/*.{ts,tsx}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/langdb/jupyterlite-langdb.git"
    },
    "scripts": {
        "build": "yarn build:lib && yarn build:labextension:dev",
        "build:prod": "yarn clean && yarn build:lib:prod && yarn build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "yarn clean:lib",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:labextension": "rimraf langdb_trace_ui/labextension langdb_trace_ui/_version.py",
        "clean:all": "yarn clean:lib && yarn clean:labextension && yarn clean:lintcache",
        "eslint": "yarn eslint:check --fix",
        "eslint:check": "eslint . --cache --ext .ts,.tsx",
        "install:extension": "yarn build",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w --sourceMap",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@floating-ui/react": "^0.26.19",
        "@jupyterlab/application": "^4.0.0",
        "@jupyterlab/apputils": "^4.3.3",
        "@jupyterlab/cells": "^4.0.0",
        "@jupyterlab/docregistry": "^4.2.3",
        "@jupyterlab/notebook": "^4.0.0",
        "@jupyterlab/observables": "^5.2.3",
        "@jupyterlab/outputarea": "^4.2.3",
        "@jupyterlab/rendermime": "^4.2.3",
        "@jupyterlab/ui-components": "^4.2.3",
        "@lumino/widgets": "^2.0.0-rc.1"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.0.0",
        "@types/json-schema": "^7.0.11",
        "@types/react": "^18.0.26",
        "@types/react-addons-linked-state-mixin": "^0.14.22",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "source-map-loader": "^1.0.2",
        "style-loader": "^3.3.1",
        "typescript": "~5.0.2",
        "yjs": "^13.5.0"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "langdb_trace_ui/labextension"
    },
    "eslintIgnore": [
        "node_modules",
        "dist",
        "coverage",
        "**/*.d.ts"
    ]
}